{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"lib": [
			"ES5",
			"ES2015",
			"ES2015.Promise",
			"DOM"
		],
		"target": "ES5",
		"outDir": "../../public",
		"module": "ES2015",
		"moduleResolution": "node",
		"jsx": "react",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"allowSyntheticDefaultImports": true,
		"declaration": false,
		"sourceMap": true
	}
}
